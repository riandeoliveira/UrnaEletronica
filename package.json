{
  "name": "urna-eletronica",
  "description": "Simulador web totalmente funcional da Nova Urna Eletrônica que foi utilizada pela primeira vez nas Eleições Brasileiras de 2022.",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "homepage": "https://urna-eletronica-three.vercel.app/",
  "main": "src/main.tsx",
  "author": {
    "name": "Rian Oliveira",
    "email": "riandiasdeoliveira2001@gmail.com"
  },
  "scripts": {
    "add:component": "node scripts/add-component.mjs",
    "add:store": "node scripts/add-store.mjs",
    "build": "npm run lint && npm run test && tsc && vite build",
    "coverage": "vitest run --coverage && start coverage/index.html",
    "cypress": "cypress open",
    "dev": "vite --open",
    "eslint": "eslint --fix src/**/*.{js,jsx,ts,tsx} vite.config.ts",
    "lint": "npm run eslint & npm run prettier & npm run stylelint",
    "prettier": "prettier --write src/**/*.{js,jsx,ts,tsx} vite.config.ts",
    "start": "vite preview --open",
    "stylelint": "stylelint --fix src/**/*.scss",
    "test": "vitest --run"
  },
  "dependencies": {
    "mobx": "^6.10.2",
    "mobx-react-lite": "^4.0.5",
    "moment": "^2.29.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-inlinesvg": "^4.1.0",
    "react-router-dom": "^6.18.0",
    "use-sound": "^4.0.1",
    "uuid": "^9.0.1",
    "vite-plugin-pwa": "^0.16.7",
    "vite-plugin-svgr": "^3.3.0",
    "workbox-window": "^7.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.36",
    "@types/react-dom": "^18.2.14",
    "@types/uuid": "^9.0.6",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "@vitejs/plugin-react-swc": "^3.4.1",
    "@vitest/coverage-v8": "^0.34.6",
    "cypress": "^13.5.0",
    "eslint": "^8.53.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jsdom": "^22.1.0",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.0.3",
    "sass": "^1.69.5",
    "stylelint": "^15.11.0",
    "stylelint-config-standard": "^34.0.0",
    "typescript": "^5.2.2",
    "typescript-plugin-css-modules": "^5.0.2",
    "vite": "^4.5.0",
    "vite-tsconfig-paths": "^4.2.1",
    "vitest": "^0.34.6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/riandeoliveira/UrnaEletronica"
  },
  "keywords": [
    "brazil",
    "clone",
    "mobx",
    "react",
    "sass",
    "typescript",
    "urna-eletronica"
  ]
}
